<?xml version="1.0" encoding="UTF-8"?>
<tomcat-users>
users=<% @node['tomcat']['users'].to_hash -%>
<% users.each do |key, value| -%>
<role rolename="<%= @node['tomcat']['role']  %>" />
<user username="<%= key %>" password="<%= value %>" roles="<%= @node['tomcat']['role'] %>" />
<% end %>
</tomcat-users>
